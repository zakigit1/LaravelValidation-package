{
    "name": "bousbaadev/lang-publisher",
    "description": "A Laravel package that publishes language folders and adds Arabic and French validation translations",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Bousbaadev\\LangPublisher\\": "src/"
        }
    },
    "authors": [
        {
            "name": "mohmmed ilyes zakaria bousbaa",
            "email": "mohammedilyeszakaria.bousbaa@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bousbaadev\\LangPublisher\\LangPublisherServiceProvider"
            ]
        }
    }
}
